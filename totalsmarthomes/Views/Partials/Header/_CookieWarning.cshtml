@* Renders The Cookie Warning across all pages on site.*@
@model StoreFront.Model.ViewModel.Resource.ResourceViewModel
<div id="ab_cookie_widget">
    @Html.Raw(Model.Disclaimer)
    <button id="ab_cookie_accept" class="search pull-right margin10" onclick="ab_cookie_accept();return false;">
        @Model.Close
    </button>
    <div class="clearfix">&nbsp;</div>
</div>
